    "#Detect floating point number\n",
    "import re\n",
    "if __name__ == \"__main__\":\n",
    "    t = int(input().strip())\n",
    "    pattern = '^[+-]?[0-9]*\\.[0-9]+$'\n",
    "    \n",
    "    for _ in range(t):\n",
    "        print(bool(re.match(pattern, input())))\n",
    "        \n",
